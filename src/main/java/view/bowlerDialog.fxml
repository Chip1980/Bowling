<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ButtonType?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>

<DialogPane fx:id="bowlersDialogPane" expanded="true" headerText="create Bowlers" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.BowlerDialogController">
   <buttonTypes>
      <ButtonType fx:constant="APPLY" />
   </buttonTypes>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <header>
      <ListView fx:id="bowlerListView" />
   </header>
   <expandableContent>
      <Label alignment="CENTER" contentDisplay="CENTER" text="-&gt; press ENTER" />
   </expandableContent>
   <content>
      <TextField fx:id="bowlerName" alignment="CENTER" promptText="insert Bowlername" />
   </content>
</DialogPane>
